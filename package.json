{
  "name": "thimble",
  "version": "1.0.0",
  "description": "Thimble game",
  "engines": {
    "node": "11.6.0"
  },
  "main": "app/server.js",
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "start": "node app/server.js",
    "client:dev": "cd app/client && npm run start",
    "client:build": "cd app/client && npm run build",
    "client:install": "cd app/client && npm install",
    "build": "npm run client:install && npm run client:build"
  },
